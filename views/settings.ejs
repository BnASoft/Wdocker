<!DOCTYPE html>
<html>
  <head>
    <% include ./partials/head %>

    <script src='/myapp/js/bundle/settings.bundle.js'></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.10/jquery.mask.js"></script>

  </head>
  <body>
    <% include ./partials/topnav %>
    <div class="container-fluid">
          <div class="row">
            <% include ./partials/nav %>
            <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
              <div class="row placeholders">
                <div class="container">
                  <form class="form-horizontal" action="/myapp/settings" method="post" >
                    <div class="form-group">
                      <label for="name" class="col-sm-2 control-label">Docker Host IP  </label>
                      <div class="col-sm-10">
                        <input type="text" class="form-control ip_address" id="ip"  name="ip" placeholder="xxx.xxx.xxx.xxx">
                      </div>
                    </div>
                    <div class="form-group">
                      <label for="name" class="col-sm-2 control-label">Docker Host Port  </label>
                      <div class="col-sm-10">
                        <input type="number" class="form-control" id="port" min="1" max="65535"   name="port" placeholder="port number">
                      </div>
                    </div>
  
                    <button type="submit" class="btn btn-default btn-primary">add</button>

                  </form>
                  <button type="button" class="btn btn-default btn-successs ping">DOCKER PING TEST</button>
                  <button type="button" class="btn btn-default btn-danger remove">DELETE</button>

                  <table data-pagination="true"  class="table table-striped jsonTable">
                  </table>
                </div>
              </div>
            </div>
          </div>
    </div>

  </body>
</html>

<!DOCTYPE html>
<html lang="ko">

<head>
  <% include ./partials/head %>
  <link href='/myapp/lib/css/bootstrap-login.css' rel='stylesheet' type='text/css'>


</head>

<body id="body">

    <div id="login">
    </div>

    <form class="form-horizontal" id="hiddenForm">
        <div class="form-group">
          <label for="term" class="col-sm-2 control-label">User: </label>
          <div class="col-sm-10">
            <input type="text" class="form-control" id="username" placeholder="user">
          </div>
        </div>
        <div class="form-group">
          <label for="term" class="col-sm-2 control-label">Password: </label>
          <div class="col-sm-10">
            <input type="password" class="form-control" id="password" placeholder="password">
          </div>
        </div>
        <div class="form-group">
          <label for="term" class="col-sm-2 control-label">Confirm Password: </label>
          <div class="col-sm-10">
            <input type="password" class="form-control" id="cpassword" placeholder="password">
          </div>
        </div>

    </form>

</body>

<% include ./partials/load %>
<script src="/myapp/lib/js/bootstrap-login.js"></script>

<script src='/myapp/js/bundle/login.bundle.js'></script>
</html>

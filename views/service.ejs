  <body>
    <% include ./partials/topnav %>
    <div class="container-fluid">
          <div class="row">
            <% include ./partials/nav %>
            <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
              <div class="row placeholders">
                <div class="container">

                      <button type="button" class="btn btn-default btn-lg ctlbtn update">Update</button>
                      <button type="button" class="btn btn-danger btn-lg ctlbtn remove">Remove</button>


                      <div id="bgrp" class="btn-group" role="group">
                              <button  onclick=" return false;"  type="button" class="btn btn-info btn-lg plus">
                                <span class="glyphicon glyphicon-plus"></span> Plus
                                </button>
                              <!-- <button  onclick=" return false;"  type="button" class="btn btn-primary btn-lg start">Start</button>
                              <button  onclick=" return false;"  type="button" class="btn btn-default btn-lg stop">Stop</button>
                              <button  onclick=" return false;"  type="button" class="btn btn-success btn-lg pause">Pause</button>
                              <button  onclick=" return false;"  type="button" class="btn btn-info btn-lg unpause">Unpause</button>
                              <button  onclick=" return false;"  type="button" class="btn btn-warning btn-lg remove">Remove</button>
                              <button  onclick=" return false;"  type="button" class="btn btn-danger btn-lg kill">Kill</button> -->
                      </div>
                      <table id="ctable" data-pagination="true" class="table table-striped jsonTable">
                      </table>

                </div>



                          <form class="form-horizontal" id="CreateService">
                              <div class="form-group">
                                <label for="serviceName" class="col-sm-2 control-label">Service Name : </label>
                                <div class="col-sm-10">
                                  <input type="text" class="form-control" id="serviceName">
                                </div>
                              </div>
                              <div class="form-group">
                                <label for="image" class="col-sm-2 control-label">Images : </label>
                                <div class="col-sm-10">
                                    <div class="btn-group">
                                      <button id="image" type="button" class="btn btn-default dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                         Images <span class="caret"></span>
                                      </button>
                                      <ul class="dropdown-menu">
                                      </ul>
                                    </div>
                                </div>
                              </div>
                              <div class="form-group">
                                <label for="command" class="col-sm-2 control-label">Command : </label>
                                <div class="col-sm-10">
                                  <input type="text" class="form-control" id="command">
                                </div>
                              </div>
                              <div class="form-group">
                                <label for="healthCheck" class="col-sm-2 control-label">HealthCheck : </label>
                                <div class="col-sm-10">
                                  <input type="text" class="form-control" id="healthCheck">
                                </div>
                              </div>

                              <div class="form-group">
                                <label for="publishedPort" class="col-sm-2 control-label">Published Port : </label>
                                <div class="col-sm-2">
                                  <input type="number" class="form-control" id="publishedPort">
                                </div>

                                <label for="targetPort" class="col-sm-2 control-label">Target Port : </label>
                                <div class="col-sm-2">
                                  <input type="number" class="form-control" id="targetPort">
                                </div>

                                <label for="protocol" class="col-sm-2 control-label">protocal : </label>
                                <div class="col-sm-2">
                                  <label class="checkbox-inline">
                                     <input type="checkbox" id="protocol" checked data-toggle="toggle" data-on="tcp"
                                     data-off="udp" data-onstyle="success" data-offstyle="primary"></input>
                                  </label>
                                </div>

                                <button class="btn btn-primary portAdd">Add</button>
                              </div>
                              <div class="form-group">
                                <div class = "portlists"></div>
                              </div>
                          </form>
              </div>


            </div>



          </div>


    </div>

  </body>
